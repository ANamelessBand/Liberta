<div class="col-md-6">  <div class="panel panel-default">    <div class="panel-heading">Новини</div>    <div class="panel-body">      <% @last_five_news.each do |entry| %>      <h4><%= entry.title %></h4>      <span><em><%= entry.date_of_publication %></em></span>      <p><%= entry.content %></p>      <br />      <% end %>    </div>  </div></div><div class="col-md-6">  <div class="panel panel-default">    <div class="panel-heading">Най-нови книги</div>    <div class="panel-body">            <%= show_print_table @last_added, false %>    </div>  </div></div>