<div class="col-md-6">  <div class="panel panel-default">    <div class="panel-heading">Новини</div>    <div class="panel-body">      <% @last_five_news.each do |entry| %>      <h4><%= entry.title %></h4>      <span><em><%= entry.date_of_publication %></em></span>      <p><%= entry.content %></p>      <br />      <% end %>    </div>  </div></div><div class="col-md-6">  <div class="panel panel-default">    <div class="panel-heading">Най-нови книги</div>    <div class="panel-body">      <table class="table table-striped table-hover table-responsive">        <thead>          <tr>            <th>Книга</th>            <th>Автор</th>            <th>Действие</th>          </tr>        </thead>        <tbody>          <tr>            <% @last_added.each do |print| %>            <td><a href="#"><%= print.title %></a></td>            <% print.authors.each do |author| %>            <td><a href="#"><%= author.name %></a></td>            <% end %>            <td>              <a href="#modal_<%=print.id%>" class="btn btn-xs btn-primary" data-toggle="modal">описание</a>              <a href="#" class="btn btn-xs btn-success"><span class="glyphicon glyphicon-plus"></span> желани</a>              <div class="modal fade" id="modal_<%=print.id%>">                <div class="modal-dialog">                  <div class="modal-content">                    <div class="modal-header">                      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>                      <h4 class="modal-title"><%= print.title %></h4>                    </div>                    <div class="modal-body">                      <p>                      <div class="pull-left from-right-1">                        <img src="./img/lord-of-the-rings-book-cover.jpg" />                      </div>                      <%= print.description %>                      </p>                    </div>                    <div class="modal-footer">                      <button type="button" class="btn btn-primary" data-dismiss="modal">Затвори</button>                    </div>                  </div>                </div>              </div>            </td>          </tr>          <% end %>        </tbody>      </table>    </div>  </div></div>